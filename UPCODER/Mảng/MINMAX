
import java.util.*;

public class Main {
    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);
        int max = -999;
        int min =9999;
        while(scanner.hasNextInt()){
            int soNguyen = scanner.nextInt();
            if(soNguyen>max){
                max=soNguyen;
            }
            if(min > soNguyen){
                min=soNguyen;
            }
        }
        System.out.println(min);
        System.out.println(max);
        System.out.println(max+min);
    }
}
