import java.util.Scanner;

public class Main {
    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);
        int soNguyen1 = scanner.nextInt();
        int soNguyen2 = scanner.nextInt();
        scanner.nextLine();
        char kyTu = scanner.next().charAt(0);
        System.out.print(soNguyen1+ "" + kyTu + "" + soNguyen2 + "=");
        switch(kyTu){
            case '+':
                System.out.println(soNguyen1+soNguyen2);
                break;
            case '-':
                System.out.println(soNguyen1-soNguyen2);
                break;
            case '*':
                System.out.println(soNguyen1*soNguyen2);
                break;
            default:
                System.out.println(soNguyen1 % soNguyen2);
        }
    }
}
